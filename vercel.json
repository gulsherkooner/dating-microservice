{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "server.js"
    },
    {
      "src": "/api/check-profile",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-profile",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-profile/:user_id",
      "dest": "server.js"
    },
    {
      "src": "/api/find-dating-profile/:_id",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-profile/:user_id",
      "dest": "server.js"
    },
    {
      "src": "/api//matches",
      "dest": "server.js"
    },
    {
      "src": "/api/wallet/topup",
      "dest": "server.js"
    },
    {
      "src": "/api/wallet/:userId",
      "dest": "server.js"
    },
    {
      "src": "/api/wallet/deduct",
      "dest": "server.js"
    },


    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/check-profile/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-profile/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-profile/:user_id/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/find-dating-profile/:_id/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-profile/:user_id/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api//matches/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/wallet/topup/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/wallet/:userId/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/wallet/deduct/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/dating-posts/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization, x-user-id",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
